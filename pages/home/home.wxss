.home-contain{
  background: #f3f3f3;
  width: 100vw;
  height: 100vh;
}
.home-tag{
  width: 100vw;
  height: 80rpx;
  border-top: 1px solid #f9f9f9;
  border-bottom: 1px solid #f9f9f9;
  flex-direction:row;
  background: #fff;
  display: flex;
  font-size: 28rpx;
}
.home-tag-word{
  margin: 0 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;

}
.home-type{
  width: 100vw;
  height: 82rpx;
  border-top: 1px solid #f9f9f9;
  border-bottom: 1px solid #f9f9f9;
  color: #0091ff;
  display: flex;
  flex-direction:row;
  margin-top: 24rpx;
  background: #fff;

}
.home-card{
  width: 100vw;
  height: 200rpx;
  /* border-top: 4px solid #f9f9f9; */
  border-bottom: 4px solid #f9f9f9;
  background: #fff;
  padding: 24rpx;
}
.card-name{
  color: rgba(0, 0, 0, .5);
  font-size: 24rpx;
  margin-top:20rpx;
}
.card-title{
  font-weight: bold;
  margin-top:20rpx;
  font-size: 28rpx;
}
.card-bottom{
  margin-top:20rpx;
  color: #666;
  font-size: 24rpx;
  display: flex;

}
.card-bottom-view{
  display: flex;
  align-items:center;
}
.card-bottom-like{
  display: flex;
  align-items:center;
  margin-left: 10rpx;
}
.card-bottom-view image{
  width: 32rpx;
  height: 28rpx;
  margin:0 5rpx;
}
.card-bottom-like image{
  width: 32rpx;
  height: 28rpx;
  margin:0 5rpx;
}
.nav-tabs {
  width: 100%;
  height: 75rpx;
  display: flex;
  background-color: #fff;
  /* margin: 128rpx 0 0 0; */
  position: fixed;
  top: 128rpx;
  z-index: 2;
} 
.tab-content {
  flex: 1;
}
.active {
  width: 100rpx;
  line-height: 80rpx;
  text-align: center;
  color: #0091ff;
  border-bottom: 2px solid #0091ff;
  font-size: 28rpx;
}
.default{
  width: 100rpx;
  line-height: 80rpx;
  text-align: center;
  color: #000;
  font-size: 28rpx;
}